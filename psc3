CREATE DATABASE filmtrackr_db;

USE filmtrackr_db;

CREATE TABLE Filmes (
    id INT PRIMARY KEY,
    titulo VARCHAR(255) NOT NULL,
    diretor VARCHAR(255),
    anoLancamento INT,
    genero VARCHAR(100),
    duracao DOUBLE,
    idioma VARCHAR(100),
    paisOrigem VARCHAR(100),
    assistido BOOLEAN,
    avaliacao DOUBLE
);
